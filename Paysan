public class Paysan extends Personne {
    
    // Création du début de partie
    public Paysan(String nom, int x, int y) {
        super(nom, x, y, 50); // 50 PV max pour un paysan
    }

    @Override
    public void attaquer() {
        System.out.println(nom + " donne un coup de fourche maladroit !");
    }
}